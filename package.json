  "type": "module",
  "devDependencies": {
    "typescript": "^4.9.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "rollup": "^3.16.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "tape": "^5.6.3",
    "micromark": "^3.1.0",
    "micromark-extension-gfm-strikethrough": "^1.0.4",
    "micromark-extension-gfm-table": "^1.0.5",
    "micromark-extension-directive": "^2.1.2",
    "micromark-extension-definition-list": "^1.3.0",
    "@types/hast": "^2.3.4",
    "hast-util-to-html": "^8.0.4",
    "hast-util-from-html": "^1.0.1",
    "hast-util-to-mdast": "^9.0.0",
    "hast-util-phrasing": "^2.0.2",
    "mdast-util-to-hast": "^12.3.0",
    "mdast-util-to-markdown": "^1.5.0",
    "mdast-util-definition-list": "^1.3.0",
    "mdast-util-directive": "^2.2.4",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-gfm-strikethrough": "^1.0.3",
    "markdown-table": "^3.0.3",
    "micromark-extension-xtable": "github:TRIAEIOU/micromark-extension-gfm-table#xtable3",
    "mdast-util-xtable": "github:TRIAEIOU/mdast-util-gfm-table#xtable",
    "micromark-extension-attention": "github:TRIAEIOU/micromark-extension-gfm-strikethrough#attention",
    "mdast-util-attention": "github:TRIAEIOU/mdast-util-gfm-strikethrough#attention",
    "hast-util-definition-list": "github:TRIAEIOU/hast-util-definition-list"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage",
    "test-ts": "ts-node --print test"
  }
}
